{
  "presets":["react", "es2015", "stage-1"],
  "plugins": [
    ["babel-plugin-inline-import", {
      "extensions": [".gql"]
    }],
    ["transform-async-to-generator"],
    ["transform-decorators-legacy"],
    ["add-module-exports"],
    ["react-hot-loader/babel"],
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ]
}