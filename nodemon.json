{
  "verbose": false,
  "restartable": "rs",
  "events": {
    "restart": "echo \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "ext": "js",
  "ignore": [
    "\\.git",
    "npm-debug.log",
    "node_modules/*",
    "node_modules/.*.*/node_modules",
    "src/assets/*"
  ],
  "watch": [
    "src/server"
  ]
}
